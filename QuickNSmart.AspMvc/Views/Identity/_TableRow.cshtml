@*@QnSBaseCode*@
@*This partial view can alternatively be used with the shared index view.*@
@model QuickNSmart.AspMvc.Models.Business.Account.AppAccess

@{
    Func<string, string> translateFor = ViewData["TranslateFor"] as Func<string, string>;
}

<tr>
    <td>
        @Html.DisplayFor(modelItem => Model.Identity.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Identity.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Identity.EnableJwtAuth)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Identity.AccessFailedCount)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Identity.State)
    </td>
    <td>
        @Html.ActionLink(translateFor("Edit"), "Edit", new { id = Model.Id }) |
        @Html.ActionLink(translateFor("Details"), "Details", new { id = Model.Id }) |
        @Html.ActionLink(translateFor("Delete"), "Delete", new { id = Model.Id })
    </td>
</tr>
